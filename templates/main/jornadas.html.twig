{% extends "main/layout.html.twig" %}
{% block head %}
    <link rel="stylesheet" href="./css/estilosJornadas.css">
    <script src="./js/scriptContacto.js"></script>
{% endblock %}
{% block titulo %}Jornadas Formativas{% endblock %}
{% block contenido %}
    <!-- Section -->
    <section class="container">
    <!-- Descripción de contacto -->
        <article style="margin-top:4%;" class="text-center">
        <div class="row Ag30">
                <div style="border-right: solid 3px #183e7d; padding:2% 3%;" class="col-4">
                    <p class="text-dark">¿Has oído alguna vez hablar<br> de la Agenda 2030?</p>
                </div>
                <div style="border-right: solid 3px #183e7d; width: 15px; padding:2% 3%;" class="col-4">
                    <p class="text-dark font-weight-bold">¿Cómo contribuimos a la<br> Agenda 2030?</p>
                </div>
                <div style="padding:2% 3%;" class="col-4">
                    <p class="text-dark">¿Conoces los Objetivos<br> de Desarrollo Sostenible?</p>
                </div>
            </div>
        </article>
    </section>
{% endblock %}